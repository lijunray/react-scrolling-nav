{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "indent": ["error", 4],
        "react/jsx-indent": [4],
        "react/jsx-indent-props": [4],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "globals": {
            "document": false
        }
    }
}
